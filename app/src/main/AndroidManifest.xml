<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="sousoum.com.repeatingalarmbug.reschedule.exact.idle.elapsed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="sousoum.com.repeatingalarmbug.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <service android:name="sousoum.com.repeatingalarmbug.AlarmService"/>
        <receiver android:name="sousoum.com.repeatingalarmbug.AlarmReceiver"/>
    </application>

</manifest>